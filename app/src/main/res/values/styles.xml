<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="StyleTitle" parent="TextAppearance.AppCompat">
        <item name="android:textSize">@dimen/_24sp</item>
        <item name="android:lineHeight">@dimen/_24sp</item>
        <item name="android:fontFamily">@font/montserrat_semi_bold</item>
        <item name="textAllCaps">true</item>
        <item name="android:textColor">@color/txt_header</item>
    </style>

    <style name="StyleButtons" parent="Widget.Material3.Button">
        <item name="android:textSize">@dimen/_16sp</item>
        <item name="android:lineHeight">@dimen/_16sp</item>
        <item name="android:fontFamily">@font/montserrat_regular</item>
        <item name="textAllCaps">true</item>
        <item name="android:textColor">@color/white</item>
        <item name="cornerRadius">@dimen/_8dp</item>
    </style>


</resources>